    "start": "webpack serve",
    "dev": "npm run start:standalone",
    "tw": "npx tailwindcss build -i ./src/styles/start.css -o ./src/styles/final.css --watch",
    "start:standalone": "webpack serve --env standalone --https && https-server-S-C cert.pem -K key.pem",
    "build:dev": "concurrently yarn:build:types yarn:build:webpack:dev",
    "build:staging": "concurrently yarn:build:types yarn:build:webpack:staging",
    "build:production": "concurrently yarn:build:types yarn:build:webpack:production",
    "build:webpack:dev": "webpack --env file=development --mode=production ",
    "build:webpack:staging": "webpack --env file=staging --mode=production",
    "build:webpack:production": "webpack --env file=production --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "prepare": "husky install",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "build:types": "tsc"
  },
